{
    "name": "plesk/ext-composer",
    "config": {
        "vendor-dir": "src/plib/vendor"
    },
    "autoload": {
        "psr-0": {
            "Composer_": [
                "modules/composer-example/library/",
                "src/plib/library/"
            ]
        },
        "psr-4": {
            "PleskExt\\Composer\\": [
                "modules/composer-example/library/Composer/",
                "src/plib/library/Composer/"
            ]
        }
    },
    "require": {
        "php": "^5.4 || ^7.0",
        "zendframework/zend-memory": "*"
    }
}
